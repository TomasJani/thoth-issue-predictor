[MASTER]
extension-pkg-whitelist=
    ujson,

load-plugins=
    pylint.extensions.bad_builtin,
    pylint.extensions.check_elif,
    pylint.extensions.comparetozero,
    pylint.extensions.docparams,
    pylint.extensions.docstyle,
    pylint.extensions.emptystring,
    pylint.extensions.mccabe,
    pylint.extensions.overlapping_exceptions,
    pylint.extensions.redefined_variable_type,

[MESSAGES CONTROL]

enable=all

disable=
    missing-module-docstring, # temporary
    missing-class-docstring, # temporary
    missing-function-docstring, # temporary
    fixme, # Remove this at the end
    unsubscriptable-object, # Pylint bug
    line-too-long, # Black will take care of this

[REPORTS]
# Template used to display messages. This is a python new-style format string
# used to format the message information. See doc for all details
msg-template='{path}:{line}: [{msg_id}({symbol}), {obj}] {msg})'

# Set the output format. Available formats are text, parseable, colorized, json
# and msvs (visual studio).You can also give a reporter class, eg
# mypackage.mymodule.MyReporterClass.
output-format=colorized
